/* pages/services/knowtest/knowtest.wxss */
/* 顶部导航列表容器 */
.top-view {
  display: flex;
  flex-direction: row;
  margin: 32rpx 32rpx;
  background-color: white;
  align-items: center;
  justify-self: center;
  border-radius: 20rpx;
}
/* 顶部单个导航栏容器 */
.top-view-col {
  width: 25%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-self: center;
  margin: 28rpx 0 12rpx 0 ;
}
/* 单个容器内部图片样式 */
.top-view-col image {
  width: 60rpx;
  height: 60rpx;
}
/* 单个容器内部文字样式 */
.top-view-col .text_view {
  font-size: 13px;
  color: #262626;
  line-height: 25px;
}