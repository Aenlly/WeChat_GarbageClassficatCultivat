/* pages/packageService/pages/services/knowtests/testhistory/testhistory.wxss */
/* 选项卡容器样式 */
.tabs-view {
  background-color: white;
  display: flex;
  flex-direction: row;
  padding-top: 20rpx;
}
/* 选项卡宽度 */
mp-tabs {
  width: 100%;
}
/* 头部样式 */
.tab-class {
  height: 50rpx;
  margin: 0 120rpx;
  text-align: center;
  border-bottom: rgba(250, 250, 250, 1) 2rpx solid;
}
/* 选项卡头部样式 */
/* 横向滚动容器 */
.swiper-class {
  height: 1390rpx;
}
/* tab内容容器 */
.tab-content {
  height: 1390rpx;
}
/* 纵向滚动容器 */
.scroll-view {
  height: 100%;
}
/* 历史容器 */
.scroll-content-view {
  height: 1390rpx;
}
/* 单个面板卡片样式 */
.panel-view {
  display: flex;
  flex-direction: row;
  margin: 32rpx;
  height: 90rpx;
  padding: 32rpx;
  border-radius: 20rpx;
  box-shadow: 0 8px 20rpx rgba(240, 240, 240, 0.6);
  background-color: white;
}
/* 时间样式 */
.time_view {
  width: 100%;
  line-height: 90rpx;
}
/* 分数样式 */
.score_view {
  width: 30%;
  line-height: 90rpx;
  text-align: right;
}
