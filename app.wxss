/**app.wxss**/
@import "/style/weui.wxss";
@import "/style/widget/weui-panel/weui-panel.wxss";

.container {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  background-color: rgba(250, 250, 250, 1);
}
.flex-wrp-col {
  padding: 0;
  margin: 0;
  width: 100%;
  flex-direction: column;
}
.panel-media {
  display: flex;
  flex-direction: row;
  border-bottom: rgba(250, 250, 250, 1) 2rpx solid;
}
.panel-media .panel-media-right {
  width: 194rpx;
  height: 194rpx;
}
